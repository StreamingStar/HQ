<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.hqzone" name="HQZone Addon Repository" version="1.0.0" provider-name="HQ Zone">
	<extension point="xbmc.addon.repository" name="HQZone Addon Repository">
		<info compressed="false">https://raw.githubusercontent.com/StreamingStar/HQ/master/addons.xml</info>
